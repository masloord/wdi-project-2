<h1>Show page</h1>
<p><%- gym.name %></p>
<p><%- gym._id %></p>

<form action= <%= "/gym/" + gym._id + "?_method=PUT" %> method="POST">
  <div class="form-group">
    <label for="name">name</label>
    <input class="form-control" type="name" name="name"
    value: "<%= gym.name %>" >
  </div>

  <div class="form-group">
    <label for="description">description</label>
    <input class="description" type="name" name="description"value: "<%= gym.description %>" >
  </div>

  <div class="form-group">
    <label for="capcity">capcity</label>
    <input class="capcity" type="Number" name="capcity"
    value: "<%= gym.capcity %>" >
  </div>

  <input class="btn btn-primary" type="submit">
</form>
