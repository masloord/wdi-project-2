<form action="/gym/new" method="POST">
  <div class="form-group">
    <label for="name">name</label>
    <input id="name" class="form-control" type="name" name="name">
  </div>

  <div class="form-group">
    <label for="description">description</label>
    <input class="form-control" id="description" rows="3" type="name" name="description">
  </div>

  <div class="form-group">
    <label for="capcity">capacity</label>
    <input id="capcity" class="capcity" type="Number" name="capcity">
  </div>

  <input class="btn btn-primary" type="submit">
</form>
