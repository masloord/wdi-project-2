<h1>edit comment  </h1>
<form action="/gym/<%= gym_id %>/review/<%= review.id %>?_method=PUT" method="POST">
  <div class="form-group">
    <label for="title">Title</label>
    <input  class="form-control" type="title" name="title"value: "<%= review.title %>">
  </div>

  <div class="form-group">
    <label for="description">Description</label>
    <input class="description" type="name" name="description"value: "<%= review.description %>">
  </div>

  <div class="form-group">
    <label for="rating">Rating</label>
    <input class="rating" type="Number" name="rating"value: "<%= review.rating %>"placeholder="Enter 1-5">
  </div>

  <div class="form-group">
    <label for="chiobuIndex">Chiobu Index</label>
    <input class="chiobuIndex" type="Number" name="chiobuIndex"value: "<%= review.chiobuIndex %>"placeholder="Enter 1-5">
  </div>

  <div class="form-group">
    <label for="hunkIndex">Hunk Index</label>
    <input class="hunkIndex" type="Number" name="hunkIndex"value: "<%= review.hunkIndex %>" placeholder="Enter 1-5">
  </div>

  <input class="btn btn-primary" type="submit">
</form>
